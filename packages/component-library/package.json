{
  "name": "component-library",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "yarn@3.0.2",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "build-storybook": "build-storybook --no-dll",
    "storybook": "start-storybook -p 6006 --no-dll",
    "build:src": "NODE_ENV=production babel ./src --out-dir ./dist --extensions \".ts,.tsx\" --ignore \"**/*.stories.tsx,**/*.stories.mdx,./src/setupTests.ts\"",
    "build:src:watch": "yarn build:src --watch",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly --outDir ./dist",
    "build:types:watch": "yarn build:types --watch",
    "build:watch": "yarn build:types:watch & yarn build:src:watch"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "styled-components": "^5.3.1",
    "styled-normalize": "^8.0.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "@babel/plugin-proposal-private-property-in-object": "^7.15.4",
    "@babel/preset-env": "^7.15.8",
    "@mdx-js/react": "^1.6.22",
    "@storybook/addon-actions": "^6.3.12",
    "@storybook/addon-docs": "^6.3.12",
    "@storybook/addon-essentials": "^6.3.12",
    "@storybook/addon-links": "^6.3.12",
    "@storybook/react": "^6.3.12",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.4.1",
    "@types/react": "^17.0.30",
    "@types/react-dom": "^17.0.9",
    "@types/styled-components": "^5.1.15",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "core-js": "^3.18.3",
    "jest": "^27.2.5",
    "react-is": "^17.0.2",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2"
  }
}
